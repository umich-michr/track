# HTTP port for Apache to Listen
APACHE_HTTP_PORT=80

# HTTPS port for Apache to Listen
APACHE_HTTPS_PORT=443

# AJP port for Apache-Tomcat communication
APACHE_AJP_PORT=8009

# HTTP port exposed by Docker
DOCKER_HTTP_PORT=9090

# HTTPS port exposed by Docker
DOCKER_HTTPS_PORT=9443

# AJP secret for secure communication (replace with a strong secret)
TOMCAT_AJP_SECRET=replace-with-your-secret

# Spring profile (e.g., development, test, production)
SPRING_PROFILES_ACTIVE=dev

# Database environment variables
DB_URL=jdbc:oracle:thin:@tcp://your-db-host:1521/your-db-service
DB_USERNAME=your-username
DB_PASSWORD=your-password
